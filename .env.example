# ====== Prominence II (Fabric 1.20.1) - ZIP mode (no API key) ======
# Copy this file to .env and adjust values as needed.

# Mandatory
EULA=TRUE

# Memory (Prominence is heavy)
MEMORY=6G
TZ=America/Santo_Domingo

# ZIP-based install (NO API KEY REQUIRED)
# Option A: Local file baked into the image (place the .zip next to Dockerfile and keep the same name)
CF_SERVER_MOD=prominence2-serverpack.zip
TYPE=CURSEFORGE

# Option B (alternative): Direct HTTP(S) link to the ZIP
# CF_SERVER_MOD=https://example.com/path/to/prominence2-serverpack.zip

# Server config (optional)
MOTD=Prominence II RPG (ZIP mode)
OPS=YourNickHere
ENABLE_WHITELIST=false
WHITELIST=Player1,Player2
MAX_PLAYERS=10
ALLOW_FLIGHT=TRUE
VIEW_DISTANCE=8
SIMULATION_DISTANCE=6

# JVM flags presets
USE_AIKAR_FLAGS=true
USE_MEOWICE_FLAGS=false

# Query (optional)
ENABLE_QUERY=true

# Optional QoL
SPAWN_PROTECTION=0
ONLINE_MODE=true
